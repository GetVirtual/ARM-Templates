{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    
    },
  "variables": {
    "VNET-WEPrefix": "10.10.11.0/24",
    "VNET-WESubnet1Name": "Subnet-1",
    "VNET-WESubnet1Prefix": "10.10.11.0/24",
    "VNET-NEPrefix": "10.10.12.0/24",
    "VNET-NESubnet1Name": "Subnet-1",
    "VNET-NESubnet1Prefix": "10.10.12.0/24"
  },
  "resources": [
    {
      "name": "VNET-WE",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "VNET-WE"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('VNET-WEPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('VNET-WESubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('VNET-WESubnet1Prefix')]"
            }
          }
        ]
      }
    },
    {
      "name": "VNET-NE",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "VNET-NE"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('VNET-NEPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('VNET-NESubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('VNET-NESubnet1Prefix')]"
            }
          }
        ]
      }
    }
     
  ]
}
